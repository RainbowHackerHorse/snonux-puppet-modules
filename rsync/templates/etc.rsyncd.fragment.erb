[<%= name -%>]
    path = <%= path %>
    comment = <%= comment %>
    read only = <%= readonly %>
    write only = <%= writeonly %>
    list = <%= list %>
    uid = <%= uid %>
    gid = <%= gid %>
    incoming chmod = <%= incoming_chmod %>
    outgoing chmod = <%= outgoing_chmod %>
    refuse options = <%= refuse_options %>
    max connections = <%= max_connections %>
    pre-xfer exec = <%= pre_xfer_exec %>

